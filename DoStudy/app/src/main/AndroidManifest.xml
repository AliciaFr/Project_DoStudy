<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alicia.dostudy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CalendarEntryDetails"/>
        <activity android:name=".ToDoListActivity"/>
        <activity android:name=".SignupActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".CalendarAddEntryActivity" />
        <activity android:name=".TimeTable.WeekActivity" />
        <activity android:name=".TimeTable.DayActivity" />
        <activity android:name=".TimeTable.CourseActivity"></activity>
    </application>

</manifest>